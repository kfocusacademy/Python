    "from nltk.stem import WordNetLemmatizer\n",
    "\n",
    "lemmatizer = WordNetLemmatizer()\n",
    "\n",
    "print(lemmatizer.lemmatize(\"running\", pos=\"v\"))  # → run\n",
    "print(lemmatizer.lemmatize(\"better\", pos=\"a\"))   # → good\n",
    "print(lemmatizer.lemmatize(\"cats\"))              # → cat"
   ]
